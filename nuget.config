<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <clear />
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
    <add key="TelerikNuGetV3" value="https://nuget.telerik.com/v3/index.json" />
	<add key="Paltarumi" value="https://pkgs.dev.azure.com/Paltarumi/SistemaAcopio/_packaging/Paltarumi/nuget/v3/index.json" />
  </packageSources>
  <packageSourceCredentials>
    <TelerikNuGetV3>
      <add key="Username" value="%FeedUsername%" />
      <add key="ClearTextPassword" value="%FeedClearTextPassword%" />
    </TelerikNuGetV3>
    <Paltarumi>
      <add key="Username" value="%PaltarumiFeedUsername%" />
      <add key="ClearTextPassword" value="%PaltarumiFeedClearTextPassword%" />
    </Paltarumi>
  </packageSourceCredentials>
</configuration>
